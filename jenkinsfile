pipeline
{
 agent any
  stages
  {
     stage('clone')
       {
        steps
          {
              checkout([$class: 'GitSCM', branches: [[name: '*/main']], doGenerateSubmoduleConfigurations: false, extensions: [], submoduleCfg: [], userRemoteConfigs: [[credentialsId: 'Git', url: 'https://github.com/rxa80330/pipeline-.git']]])
          }
        }
      stage('build')
       {
          steps
          {
             echo 'mvn build'
          }
       }
      stage('test')
       {
          steps
          {
             echo 'mvn test1'
          }
       }      
      stage('build && SonarQube analysis') 
       {
            steps 
            {
                
              echo 'sonar analysis'
            }
       }    
                         
        stage('docker image')
        {
            steps
            {
             echo 'docker build images'
            }
        }                       
    }
    post {
        always {
            echo 'This will always run'
        }
        success {
            echo 'This will run only if successful'
        }
        failure {
            echo 'This will run only if failed'
        }
        unstable {
            echo 'This will run only if the run was marked as unstable'
        }
        changed {
            echo 'This will run only if the state of the Pipeline has changed'
            echo 'For example, if the Pipeline was previously failing but is now successful'
        }
   }    
}
  

